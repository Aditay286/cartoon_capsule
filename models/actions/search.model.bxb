action (search) {
  type(Search)
  description (searches the worlds largest cartoon engine)
  collect {
    input (keyword) {
      type (Keyword)
      min (Optional) max (One)
    }
    input (category) {
      type (Category)
      min (Optional) max (One)
    }
    input (author) {
      type (Author)
      min (Optional) max (One)
    }

    
  }
  output (Cartoon)
}